<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="HatslogicPageOptimisation\Command\Generate\CriticalCss">
            <argument type="service" id="hatslogic.critical_assets.service.critical_css_generation_service" />

            <tag name="console.command"/>
        </service>

        <service id="HatslogicPageOptimisation\Command\Generate\ConvertImagesToWebpCommand">
             <argument type="service" id="HatslogicPageOptimisation\Service\ConvertImagesToWebpService" />
            <tag name="console.command" />
        </service>
    </services>
</container>

