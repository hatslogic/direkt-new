           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="hatslogic.critical_assets.processor.critical_css_processor"
                 class="HatslogicPageOptimisation\Processor\CriticalCssProcessor"
                 public="true">
