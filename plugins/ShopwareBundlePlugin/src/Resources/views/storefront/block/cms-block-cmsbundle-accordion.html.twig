{% block block_cmsbundle_accordion %}
	<div class="cms-block-cmsbundle-accordion">
	<div class="position-relative">
		{% block block_cmsbundle_accordion_title %}
			{% set element = block.slots.getSlot('title') %}
			<div class="container py-2">
				<div class="row">
					<div class="col-md-12">
						{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing  %}
					</div>
				</div>
			</div>
		{% endblock %}
		{% block block_cmsbundle_accordion_content %}
			{% set element = block.slots.getSlot('accordion') %}
			<div class="container py-2">
				<div class="row">
					<div class="col-md-12">
						{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing  %}
					</div>
				</div>
			</div>
		{% endblock %}
		{% block block_cmsbundle_accordion_background %}
			<div class="accordion__background {% if isBgImage %} position-absolute top-0 bottom-0 start-0 w-100 {% else %}position-absolute {% endif %}  top-0 start-0 w-100 h-100" style="z-index: -2">
				{% set element = block.slots.getSlot('background') %}
				{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
			</div>
		{% endblock %}
		</div>
	</div>
{% endblock %}
