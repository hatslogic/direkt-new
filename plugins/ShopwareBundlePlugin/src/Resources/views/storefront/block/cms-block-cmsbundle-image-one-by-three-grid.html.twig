{% block block_cmsbundle_one_by_three_grid %}
<div class="cms-block-cmsbundle-three-column-grid cmsbundle-custom">
    <div class="container">
        <div class="row">
            {% block block_cmsbundle_one_by_three_grid_left %}
                {% set element = block.slots.getSlot('left') %}
                <div class="col-md-6 pb-4 pb-md-0">
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                </div>
            {% endblock %}
            <div class="col-md-6">
                <div class="row">
                    {% block block_cmsbundle_one_by_three_grid_right_top %}
                        {% set element = block.slots.getSlot('right-top') %}
                        <div class="col-12 mb-2">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    {% endblock %}
                    {% block block_cmsbundle_one_by_three_grid_right_middle %}
                        {% set element = block.slots.getSlot('right-middle') %}
                        <div class="col-12 mb-2">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    {% endblock %}
                    {% block block_cmsbundle_one_by_three_grid_right_bottom %}
                        {% set element = block.slots.getSlot('right-bottom') %}
                        <div class="col-md-12">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}
