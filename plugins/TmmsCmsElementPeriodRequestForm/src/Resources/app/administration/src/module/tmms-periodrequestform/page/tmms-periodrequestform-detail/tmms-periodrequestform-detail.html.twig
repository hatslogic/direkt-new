{% block periodrequestform_detail %}
    <sw-page class="periodrequestform-detail">
        {% block periodrequestform_detail_actions %}
            <template #smart-bar-actions>
                {% block periodrequestform_detail_actions_cancel %}
                    <sw-button
                        v-tooltip.bottom="tooltipCancel"
                        @click="onCancel">
                            {{ $t('periodrequestform.detail.cancelButtonText') }}
                    </sw-button>
                {% endblock %}

                {% block periodrequestform_detail_actions_save %}
                    <sw-button-process
                        v-tooltip.bottom="tooltipSave"
                        class="periodrequestform-detail__save-action"
                        :is-loading="isLoading"
                        :process-success="processSuccess"
                        :disabled="isLoading || !acl.can('periodrequestform.editor')"
                        variant="primary"
                        @update:process-success="onSaveFinish"
                        @click="onSave">
                            {{ $t('periodrequestform.detail.saveButtonText') }}
                    </sw-button-process>
                {% endblock %}
            </template>
        {% endblock %}

        {% block sw_settings_user_detail_content %}
            <template #content>
                <sw-card-view>
                    <template v-if="isLoading">
                        <sw-skeleton />
                        <sw-skeleton />
                    </template>

                    <template v-else>
                        <sw-card
                            v-if="periodrequestform"
                            position-identifier="periodrequestform-detail"
                            :is-loading="isLoading"
                        >
                            <div
                                v-if="periodrequestform"
                                class="periodrequestform-detail__grid"
                            >
                                <sw-text-field
                                    v-model:value="periodrequestform.salutation"
                                    name="sw-field--periodrequestform-salutation"
                                    class="periodrequestform-detail__grid-salutation"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.salutationLabel')"/>

                                <sw-text-field
                                    v-model:value="periodrequestform.firstname"
                                    name="sw-field--periodrequestform-firstname"
                                    class="periodrequestform-detail__grid-firstname"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.firstnameLabel')"/>

                                <sw-text-field
                                    v-model:value="periodrequestform.lastname"
                                    name="sw-field--periodrequestform-lastname"
                                    class="periodrequestform-detail__grid-lastname"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.lastnameLabel')"/>

                                <sw-text-field
