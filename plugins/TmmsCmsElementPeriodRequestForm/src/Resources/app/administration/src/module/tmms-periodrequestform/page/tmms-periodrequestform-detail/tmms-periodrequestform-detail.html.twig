                                    class="periodrequestform-detail__grid-street"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.streetLabel')"/>

                                <sw-text-field
                                    v-model:value="periodrequestform.zipcode"
