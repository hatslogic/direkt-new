                                    class="periodrequestform-detail__grid-zipcode"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.zipcodeLabel')"/>

                                <sw-text-field
                                    v-model:value="periodrequestform.city"
                                    name="sw-field--periodrequestform-city"
                                    class="periodrequestform-detail__grid-city"
                                    :disabled="!acl.can('periodrequestform.editor')"
                                    :label="$t('periodrequestform.detail.cityLabel')"/>

                                <sw-text-field
