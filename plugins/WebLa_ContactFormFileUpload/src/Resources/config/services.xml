<?xml version="1.0"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="WebLa_ContactFormFileUpload\Storefront\Controller\FormControllerDecorator" decorates="Shopware\Storefront\Controller\FormController" public="true">
            <argument type="service" id="WebLa_ContactFormFileUpload\Storefront\Controller\FormControllerDecorator.inner" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileNameProvider" />
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
            <call method="setTwig">
                <argument type="service" id="twig"/>
            </call>
        </service>
    </services>
</container>